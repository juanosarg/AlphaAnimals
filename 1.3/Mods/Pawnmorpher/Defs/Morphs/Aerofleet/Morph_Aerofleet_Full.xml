<Defs>
	<HediffDef ParentName="NewHerbavourMorphTfBase">
		<defName>AA_PawnmorphAerofleetTF</defName>
		<label>aerofleetmorph</label>
		<description>This person has mechanites in their blood that are slowly mutating them into an aerofleet. Keep an eye on them, as leaving the mechanites unattended could lead to them becoming far more gasbag than human.</description>
		<stages>
			<li Class="Pawnmorph.Hediffs.FullTransformationStage">
				<label>reeling</label>
				<hungerRateFactor>1.6</hungerRateFactor>
				<painOffset>0.05</painOffset>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<pawnkinds>
					<li>AA_Aerofleet</li>
				</pawnkinds>
				<transformationTale>TransformationMisc</transformationTale>
			</li>
			<li Class="Pawnmorph.Hediffs.TransformationStage">
				<label>transforming</label>
				<minSeverity>0.6</minSeverity>
				<painOffset>0.1</painOffset>
				<vomitMtbDays>1</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Sight</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<letterText>TransformationStageWarningContent</letterText>
				<letterLabel>TransformationStageWarningLabel</letterLabel>
			</li>
			<li Class="Pawnmorph.Hediffs.MorphTransformationStage">
				<label>changing</label>
				<minSeverity>0.65</minSeverity>
				<painOffset>0.1</painOffset>
				<vomitMtbDays>1</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Sight</capacity>
						<offset>-0.05</offset>
					</li>
					<li>
						<capacity>Manipulation</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<morph>AA_AerofleetMorph</morph>
			</li>
		</stages>
	</HediffDef>
</Defs>